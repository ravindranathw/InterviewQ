@model IEnumerable<InterviewQ.MVC.Models.TestModel>

@{
    ViewBag.Title = "Index";
}

<h2>Available Tests</h2>
<p>Found @Model.Count() Tests</p>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.TestName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.TestName)
        </td>
        <td>
            @Html.ActionLink("Select", "Index", new { controller = "Test", id=item.TestID})
        </td>
    </tr>
}

</table>
